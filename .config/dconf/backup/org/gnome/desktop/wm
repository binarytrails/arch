[preferences]
button-layout='appmenu:minimize,maximize,close'
resize-with-right-button=true

[keybindings]
switch-panels=['<Primary><Alt>s']
toggle-maximized=@as []
maximize=['<Alt>Up']
unmaximize=['<Alt>Down']
minimize=['<Alt>h']
close=['<Alt>q']
begin-move=['<Alt>d']
begin-resize=['<Alt>r']
cycle-group=['<Shift><Alt>Tab']
cycle-group-backward=@as []
move-to-workspace-up=['<Shift><Alt>Page_Up']
move-to-workspace-down=['<Shift><Alt>Page_Down']
